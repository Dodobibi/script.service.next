msgctxt "#30004"
msgid "Advanced"
msgstr "Avancé"

msgctxt "#30005"
msgid "Default action when nothing selected"
msgstr "Action par défaut lorsque rien n'est sélectionné"

msgctxt "#30006"
msgid "Watch Now"
